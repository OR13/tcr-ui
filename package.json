{
  "name": "tcr-ui",
  "author": "kangarang",
  "version": "0.3.6",
  "description": "A client-side shell to interact with token-curated registries",
  "repository": "https://github.com/kangarang/tcr-ui",
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.0.0"
  },
  "private": true,
  "dependencies": {
    "@aragon/ui": "0.5.0",
    "babel-polyfill": "6.26.0",
    "bn.js": "4.11.8",
    "date-fns": "1.29.0",
    "ethers": "3.0.4",
    "express": "4.16.2",
    "file-saver": "1.3.3",
    "history": "4.7.2",
    "husky": "0.14.3",
    "immutable": "3.8.2",
    "ipfs-api": "19.0.0",
    "ipfs-mini": "1.1.2",
    "jazzicon": "1.5.0",
    "lint-staged": "7.0.0",
    "lodash": "4.17.4",
    "material-ui": "^1.0.0-beta.36",
    "moment": "2.20.1",
    "path": "0.12.7",
    "prettier": "1.11.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-json-tree": "0.11.0",
    "react-markdown": "3.1.4",
    "react-motion": "0.5.2",
    "react-redux": "5.0.6",
    "react-scripts": "1.0.17",
    "redux": "3.7.2",
    "redux-immutable": "4.0.0",
    "redux-logger": "3.0.6",
    "redux-saga": "0.16.0",
    "reselect": "3.0.1",
    "styled-components": "2.3.0"
  },
  "scripts": {
    "start": "NODE_PATH=./src node server.js",
    "start:prod": "npm run build && NODE_PATH=./src node server.js",
    "dev": "npm run lint ./src && NODE_PATH=./src react-scripts start",
    "build": "npm run lint ./src && NODE_PATH=./src react-scripts build",
    "test": "npm run lint ./src && NODE_PATH=./src react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "precommit": "lint-staged",
    "fix": "eslint --fix ./src",
    "lint": "eslint ./src"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-jest": "22.4.1",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "eslint": "4.18.1",
    "eslint-plugin-react": "7.7.0",
    "react-test-renderer": "16.2.0",
    "regenerator-runtime": "0.11.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "plugins": [
      "react"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "arrow-body-style": 0,
      "semi": 0
    },
    "parser": "babel-eslint"
  }
}
