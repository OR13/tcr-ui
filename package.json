{
  "name": "tcr-ui",
  "author": "kangarang",
  "version": "0.3.5",
  "description": "A client shell to transact with Token-Curated Registries",
  "repository": "https://github.com/kangarang/tcr-ui",
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.0.0"
  },
  "private": true,
  "dependencies": {
    "@aragon/ui": "0.5.0",
    "babel-polyfill": "6.26.0",
    "bluebird": "3.5.1",
    "bn.js": "4.11.8",
    "ethers": "3.0.4",
    "express": "4.16.2",
    "file-saver": "1.3.3",
    "history": "4.7.2",
    "immutable": "3.8.2",
    "ipfs-api": "18.1.1",
    "jazzicon": "1.5.0",
    "jsonfile": "4.0.0",
    "lodash": "4.17.4",
    "material-ui": "^1.0.0-beta.36",
    "moment": "2.20.1",
    "path": "0.12.7",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-json-tree": "0.11.0",
    "react-markdown": "3.1.4",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "react-scripts": "1.0.17",
    "redux": "3.7.2",
    "redux-immutable": "4.0.0",
    "redux-logger": "3.0.6",
    "redux-saga": "0.16.0",
    "reselect": "3.0.1",
    "semantic-ui-css": "2.3.0",
    "semantic-ui-react": "0.78.3",
    "styled-components": "2.3.0"
  },
  "scripts": {
    "start": "node server.js",
    "start:prod": "npm run build && node server.js",
    "dev": "npm run lint ./src && NODE_PATH=./src react-scripts start",
    "build": "npm run lint ./src && NODE_PATH=./src react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "fix": "eslint --fix",
    "lint": "eslint"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "8.2.2",
    "babel-jest": "22.4.1",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "eslint": "4.18.1",
    "eslint-plugin-react": "7.7.0",
    "jest": "22.4.2",
    "jest-cli": "22.4.2",
    "react-test-renderer": "16.2.0",
    "regenerator-runtime": "^0.11.1"
  },
  "eslintConfig": {
    "plugins": [
      "react"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "arrow-body-style": 0,
      "semi": 0
    },
    "parser": "babel-eslint"
  }
}
