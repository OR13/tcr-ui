{
  "name": "tcr-ui",
  "author": "kangarang",
  "version": "0.3.6",
  "description": "A client-side shell to interact with token-curated registries",
  "repository": "https://github.com/kangarang/tcr-ui",
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.0.0"
  },
  "private": true,
  "dependencies": {
    "@aragon/ui": "0.5.0",
    "babel-polyfill": "6.26.0",
    "bn.js": "4.11.8",
    "date-fns": "1.29.0",
    "ethers": "^3.0.8",
    "express": "4.16.2",
    "file-saver": "1.3.3",
    "history": "4.7.2",
    "husky": "0.14.3",
    "ipfs-api": "19.0.0",
    "ipfs-mini": "1.1.2",
    "jazzicon": "1.5.0",
    "lint-staged": "7.0.0",
    "lodash": "4.17.4",
    "material-ui": "^1.0.0-beta.40",
    "moment": "2.20.1",
    "path": "0.12.7",
    "prettier": "1.11.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-json-tree": "0.11.0",
    "react-markdown": "3.1.4",
    "react-motion": "0.5.2",
    "react-redux": "5.0.6",
    "react-scripts": "1.1.4",
    "redux": "3.7.2",
    "redux-logger": "3.0.6",
    "redux-saga": "0.16.0",
    "reselect": "3.0.1",
    "styled-components": "2.3.0"
  },
  "scripts": {
    "start": "NODE_PATH=./src node server.js",
    "start:prod": "npm run build && NODE_PATH=./src node server.js",
    "dev": "npm run lint && NODE_PATH=./src react-scripts start",
    "build": "NODE_PATH=./src react-scripts build",
    "test": "npm run lint && NODE_PATH=./src react-scripts test",
    "eject": "react-scripts eject",
    "precommit": "lint-staged",
    "fix": "eslint --fix ./src",
    "lint": "eslint ./src"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-jest": "22.4.1",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "eslint": "4.18.1",
    "eslint-plugin-lodash-fp": "2.1.3",
    "eslint-plugin-react": "7.7.0",
    "react-test-renderer": "16.2.0",
    "regenerator-runtime": "0.11.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "plugins": [
      "react",
      "lodash-fp"
    ],
    "env": {
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "lodash-fp/consistent-compose": "off",
      "lodash-fp/consistent-name": [
        "error",
        "_"
      ],
      "lodash-fp/no-argumentless-calls": "error",
      "lodash-fp/no-chain": "error",
      "lodash-fp/no-extraneous-args": "error",
      "lodash-fp/no-extraneous-function-wrapping": "warn",
      "lodash-fp/no-extraneous-iteratee-args": "error",
      "lodash-fp/no-for-each": "off",
      "lodash-fp/no-partial-of-curried": "error",
      "lodash-fp/no-single-composition": "error",
      "lodash-fp/no-submodule-destructuring": "error",
      "lodash-fp/no-unused-result": "error",
      "lodash-fp/prefer-compact": "error",
      "lodash-fp/prefer-composition-grouping": "error",
      "lodash-fp/prefer-constant": [
        "error",
        {
          "arrowFunctions": false
        }
      ],
      "lodash-fp/prefer-flat-map": "error",
      "lodash-fp/prefer-get": "error",
      "lodash-fp/prefer-identity": [
        "error",
        {
          "arrowFunctions": false
        }
      ],
      "lodash-fp/preferred-alias": "off",
      "lodash-fp/use-fp": "error",
      "arrow-body-style": 0,
      "semi": 0
    },
    "parser": "babel-eslint"
  }
}
